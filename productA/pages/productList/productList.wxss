page,
.page,
.page__bd{
    height: 100%;
}
.page__bd{
    padding-bottom: 0;
}
page{
	background-color: #f5f5f5;
}
.weui-navbar{
	margin-bottom: 20rpx;
}

.weui-navbar{
	border-bottom:none;
	height: 68rpx;
	line-height: 68rpx;
	position: fixed;
	background-color: #fff;
	/*overflow-x: auto;
	-webkit-overflow-scrolling:touch;*/
    justify-content: space-between;
}
.weui-navbar__item{
	padding: 0;
	color: #9E9E9E;
	/*flex-basis: 150rpx;
  	flex-shrink: 0;
  	white-space: nowrap;*/
}
.weui-bar__item_on{
	color: #282828!important;
}
.weui-navbar__slider{
	width: 20px;
	background-color: #FFA600;
}
@import "/static/css/productListsStyleCommon.wxss";

.poroductLists{
	padding-top: 88rpx;
	background-color: #f5f5f5;
	padding-bottom: 20rpx;
}
.poroductLists:last-child{
	padding-bottom: 0;
}
.product-item{
	box-shadow: none;
	background-color: #fff;
	padding-left:40rpx;
	padding-right:40rpx;
	margin-bottom: 0;
	border-radius: 0;
}
.youxuan,.rexiao{
	background-color: #fff;
	padding: 40rpx 40rpx 0;
	font-size:36rpx;
	line-height: 44rpx;
}
.sale-num{
	font-size:24rpx;
	color:rgba(158,158,158,1);
	line-height:30rpx;
}
.product-right-bottom{
	display: flex;
	flex-flow: row nowrap;
	justify-content: space-between;
	align-items: flex-end;
}
.product-right-price{
	display: flex;
	flex-flow: row nowrap;
	align-items: flex-end;
}

.rexiaoLists{
	padding-top: 0;
}
.youxuanLists .product-item:last-child,.rexiaoLists .product-item:last-child{
	border-bottom: none;
}